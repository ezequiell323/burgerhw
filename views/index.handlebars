
<h1>burgers!</h1>

<h2>Burgers !</h2>
<div class="container">
    <div class="row">
        <div class="col-12">
         <ul>
  {{#each BURGER}}
    {{#unless this.DEVOURED}}
    <h2>{{this.id}}. {{this.BURGER_NAME}}</h2>
    {{/unless}}
  {{/each}}
</ul>
        </div>
    </div>

<div class="row">
        <div class="col-12">
<h2>Devoured!</h2>

<ul>
  {{#each BURGER}}
    {{#if this.DEVOURED}}
     <h2>{{this.id}}. {{this.BURGER_NAME}}</h2>
    {{/if}}
  {{/each}}
</ul>
</div>
    </div>
<div class="row">
        <div class="col-12">
<h2>Burgers</h2>
<form class="create-form">

  <div class="form-group">
    <label for="ca">BURGER Name:</label>
    <input type="text" id="ca" name="name">
  </div>
</div>
    </div>
  

  <button type="submit">Burger</button>
</form>
